{
  "name": "webdav",
  "displayName": "WebDAV Upload",
  "description": "Uploads local to a remote host using WebDAV",
  "version": "0.0.1",
  "publisher": "Jorith van den Heuvel",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.webdavUpload",
        "title": "WebDAV: Upload"
      },
      {
        "command": "extension.webdavCompare",
        "title": "WebDAV: Compare"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.0",
    "eslint": "^4.6.1",
    "find-config": "^1.0.0",
    "mocha": "^3.5.0",
    "path": "^0.12.7",
    "tmp": "0.0.33",
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "webdav-fs": "^1.5.0"
  }
}
